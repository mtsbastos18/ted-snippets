{
	"name": "tedsenac",
	"displayName": "TedSenac",
	"description": "Extensao criada para facilitar a diagramação de conteúdos do TED. ",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:html",
		"onCommand:tedsenac.criaHTML"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tedsenac.criaHTML",
				"title": "Criar HTML"
			}
		],
		"snippets": [
			{
			  "language": "html",
			  "path": "./snippets/impressao.code-snippets"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
